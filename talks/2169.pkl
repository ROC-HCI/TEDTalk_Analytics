 "more_resources": [{"status": "approved", "start_at": null, "headline": "Gandhi\'s not my dad", "link_url": "http://blog.ted.com/2015/01/30/ted-fellow-bassam-tariq-upends-conventional-views-of-muslim-life/", "eyebrow": null, "end_at": null, "image_url": "", "published": true, "visible_url": "", "type": "from_the_blog", "blurb": "Bassam Tariq tells his origin story: what it was like moving from Karachi to Queens to Houston, and how he got tired of telling other kids he wasn\'t related to Gandhi."}], "hero": "https://pe.tedcdn.com/images/ted/ead696e0726a646b206200ce357800d627b170c2_2880x1620.jpg", "description": "Bassam Tariq is a blogger, a filmmaker, and a halal butcher -- but one thread unites his work: His joy in the diversity, the humanness of our individual experiences. In this charming talk, he shares clips from his film \\"These Birds Walk\\" and images from his tour of 30 mosques in 30 days -- and reminds us to consider the beautiful complexity within us all.", "tags": ["TED Fellows", "creativity", "film", "writing"], "downloads": {"languages": [{"languageCode": "ar", "endonym": "\\u0627\\u0644\\u0639\\u0631\\u0628\\u064a\\u0629", "isRtl": true, "ianaCode": "ar", "languageName": "Arabic"}, {"languageCode": "bn", "endonym": "\\u09ac\\u09be\\u0982\\u09b2\\u09be", "isRtl": false, "ianaCode": "bn", "languageName": "Bengali"}, {"languageCode": "ca", "endonym": "Catal\\u00e0", "isRtl": false, "ianaCode": "ca", "languageName": "Catalan"}, {"languageCode": "zh-cn", "endonym": "\\u4e2d\\u6587 (\\u7b80\\u4f53)", "isRtl": false, "ianaCode": "zh-Hans", "languageName": "Chinese, Simplified"}, {"languageCode": "zh-tw", "endonym": "\\u4e2d\\u6587 (\\u7e41\\u9ad4)", "isRtl": false, "ianaCode": "zh-Hant", "languageName": "Chinese, Traditional"}, {"languageCode": "hr", "endonym": "Hrvatski", "isRtl": false, "ianaCode": "hr", "languageName": "Croatian"}, {"languageCode": "cs", "endonym": "\\u010ce\\u0161tina", "isRtl": false, "ianaCode": "cs", "languageName": "Czech"}, {"langu